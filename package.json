  },
  "devDependencies": {
    "chokidar-cli": "^2.1.0",
    "ejs": "^2.6.2",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-plugin-ejs": "0.0.2",
    "eslint-plugin-import": "^2.20.1",
    "fs-extra": "^8.1.0",
    "glob": "^7.1.6",
    "npm-run-all": "^4.1.5",
    "serve": "^11.3.0"
  }
}
